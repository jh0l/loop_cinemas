{
  "ts-node": {
    "esm": true
  },
  "include": ["./server/**/*"],
  "compilerOptions": {
    "esModuleInterop": true,
    "isolatedModules": true,
    "lib": ["esnext"],
    "module": "ESNext",
    "moduleResolution": "NodeNext",
    "noImplicitAny": false,
    "outDir": "./dist",
    "resolveJsonModule": true,
    "strict": false,
    "strictNullChecks": false,
    "target": "ES2016",
    "allowSyntheticDefaultImports": true
  }
}
